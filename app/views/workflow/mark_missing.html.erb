<h4>Scan an IU Barcode to Mark Missing</h4>
<%= form_for 'physical_object', url: process_mark_missing_path do |f| %>
	<table class="_50">
		<tr>
			<th>IU Barcode</th>
			<td><%= f.text_field :iu_barcode, {class: 'iu_barcode'} %></td>
		</tr>
		<tr>
			<th colspan="2">Comment</th>
		</tr>
		<tr>
			<td colspan="2">
				<%= text_area_tag :comment %>
			</td>
		</tr>
		<tr>
			<td colspan="2"><%= f.submit 'Mark Missing' %> </td>
		</tr>
	</table>
<% end %>
